    "import math\n",
    "from typing import List\n",
    "\n",
    "def rastrigin(x: List[float]) -> float:\n",
    "    A = 10.0\n",
    "    n = len(x)\n",
    "    l = [xi**2 - A * math.cos(2 * math.pi * xi) for xi in x]\n",
    "    return n * A + sum(l)\n",
    "\n",
    "# Example usage\n",
    "x = [0]*10000000\n",
    "result = rastrigin(x)\n",
    "print(result)"
   ]
